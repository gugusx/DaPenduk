<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.guguscode.dapenduk">

    <uses-permission android:name="android.permission.CAMERA" />
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcherdapenduk"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcherdapendukround"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".Activities.EditData"></activity>
        <activity android:name=".Activities.LihatData" />
        <activity android:name=".Activities.TambahData" />
        <activity android:name=".Activities.AdminLogin" />
        <activity android:name=".HomePage" />
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <meta-data
            android:name="preloaded_fonts"
            android:resource="@array/preloaded_fonts" />
    </application>

</manifest>